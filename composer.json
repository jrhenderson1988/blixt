{
    "name": "jrhenderson1988/blixt",
    "description": "A search engine, inspired by Apache Lucene, written in PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathon Henderson"
        }
    ],
    "require": {
        "php": ">= 7.1.0",
        "ext-mbstring": "*",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Blixt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlixtTests\\": "tests/"
        }
    }
}
